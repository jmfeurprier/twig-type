{
    "name": "jmf/package-template",
    "description" : "Package template for jmf packages",
    "license": "mit",
    "type": "library",
    "require": {
        "php": ">=8.3"
    },
    "require-dev": {
        "phing/phing": "^2.17",
        "phpmd/phpmd": "^2.13",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^11.1",
        "rector/rector": "^1.0",
        "squizlabs/php_codesniffer": "^3.8"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Jmf\\_PackageTemplate_\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jmf\\_PackageTemplate_\\": "tests/unit/src"
        }
    }
}
